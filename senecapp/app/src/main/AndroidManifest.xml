<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tta.intel.eus.senecapp">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentador.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentador.LoginActivity" />
        <activity android:name=".presentador.RegisterActivity" />
        <activity android:name=".presentador.LoginMenuActivity" />
        <activity android:name=".presentador.Register2MenuActivity" />
        <activity android:name=".presentador.Register2HombreActivity" />
        <activity android:name=".presentador.MenuActivity" />
        <activity android:name=".presentador.Register2MujerActivity" />
        <activity android:name=".presentador.ActividadesActivity" />
        <activity android:name=".presentador.ExpresionesActivity" />
        <activity android:name=".presentador.MapasActivity" />
        <activity android:name=".presentador.AjustesActivity" />
        <activity android:name=".presentador.Juego1Activity" />
        <activity android:name=".presentador.Juego2Activity" />
        <activity android:name=".presentador.Expresiones1Activity" />
        <activity android:name=".presentador.Expresiones2Activity" />
        <activity android:name=".presentador.Expresiones3Activity"></activity>
    </application>

</manifest>